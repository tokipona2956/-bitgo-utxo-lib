    "bs58check": "^2.0.0",
    "cashaddress": "^1.1.0",
    "typeforce": "^1.11.3",
    "varuint-bitcoin": "^1.0.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
