    "debug": "^3.1.0",
    "fs-extra": "^9.1.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "gitHead": "321f96e8abb546d18c046f224d1f082319e4b38f",
